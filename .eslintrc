{
    "rules": {
        "no-console": "off",
        "indent": [
            "error", 
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
     "globals": {
        "it": false,
        "describe": false
     }
}
